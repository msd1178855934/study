<view class="mine-profile">
    <view class="mine-profile-avatar">
        <open-data style="position: absolute; z-index: 999" type="userAvatarUrl"></open-data>
    </view>
    <view>
        <open-data class="mine-profile-nickname name" type="userNickName"></open-data>
        <view class="phone">{{userInfo.phone}}</view>
    </view>
</view>
<view class="mine__not-log-auth" wx:if="{{!userInfo.isAuth}}">
    <image class="mine__not-log-auth-icon" mode="scaleToFill" src="https://imgcache.gzonline.gov.cn/cos/smrz_6a057627.svg"></image>
    <view class="mine__not-log-auth-title">{{authTitle}}</view>
    <view class="mine__not-log-auth-info">
        <view>
            <text>{{authDesc}}</text>
        </view>
    </view>
    <button bindgetphonenumber="onTapLogin" class="mine__btn" loading="{{needCheckSession}}" openType="getPhoneNumber" type="primary">
      {{authBtnText}}
    </button>
</view>
<block wx:if="{{userInfo.isAuth}}">
    <view class="mine__list">
        <g-cell access="true" to="{{isRealName?'/packages/health-code/pages/realname-info/index':'/packages/health-code/pages/realname-submit/index'}}" wx:if="{{!userInfo.isLeader&&language.isvisible}}">
            <view slot="prepend">
                <image class="mine__cell-image" src="https://imgcache.gzonline.gov.cn/cos/iconspace_card_26438e10.svg"></image>
            </view>
            <view class="row" slot="content">
                <view class="label">{{language.realNameAuthentication}}</view>
                <view class="tipsNo" wx:if="{{!isRealName}}">
                    <view>{{language.unverified}}</view>
                    <image class="tipsIcon" src="https://imgcache.gzonline.gov.cn/cos/readname-no_8ae67657.png"></image>
                </view>
                <view class="tipsNo" wx:if="{{isRealName}}">
                    <view style="color:#000">{{language.verified}}</view>
                    <image class="tipsIcon" src="https://imgcache.gzonline.gov.cn/cos/readname_5074e506.png"></image>
                </view>
            </view>
        </g-cell>
        <g-cell access="true" label="{{language.myToDo}}" to="/pages/mine/todo/list/list?todoType=affair" wx:if="{{userInfo.isVolunteer&&!userInfo.isLeader}}">
            <view slot="prepend">
                <image class="mine__cell-image" src="https://imgcache.gzonline.gov.cn/cos/iconspace_db_d9c183a2.svg"></image>
            </view>
        </g-cell>
        <g-cell access="true" label="{{language.myHealthSelfReport}}" to="/pages/mine/todo/list/list?todoType=check" wx:if="{{!userInfo.isLeader}}">
            <view slot="prepend">
                <image class="mine__cell-image" src="https://imgcache.gzonline.gov.cn/cos/iconspace_sb_cc481086.svg"></image>
            </view>
        </g-cell>
        <g-cell access="true" label="{{language.myNews}}" to="/pages/mine/empty/index?title={{language.myNews}}" wx:if="{{userInfo.isLeader}}">
            <view slot="prepend">
                <image class="mine__cell-image" src="https://imgcache.gzonline.gov.cn/cos/iconspace_bell_fe716069.svg"></image>
            </view>
        </g-cell>
        <g-cell access="true" label="{{language.myToDo}}" to="/pages/mine/empty/index?title={{language.myToDo}}" wx:if="{{userInfo.isLeader}}">
            <view slot="prepend">
                <image class="mine__cell-image" src="https://imgcache.gzonline.gov.cn/cos/iconspace_db_d9c183a2.svg"></image>
            </view>
        </g-cell>
        <g-cell access="true" label="{{language.myClue}}" to="/pages/mine/todo/list/list?todoType=report" wx:if="{{cityName!=='深圳市'&&!userInfo.isLeader&&language.isvisible}}">
            <view slot="prepend">
                <image class="mine__cell-image" src="https://imgcache.gzonline.gov.cn/cos/iconspace_bs_f57a8fca.svg"></image>
            </view>
        </g-cell>
        <g-cell access="true" label="{{language.mydonation}}" to="/pages/mine/donation/index" wx:if="{{false}}">
            <view slot="prepend">
                <image class="mine__cell-image" src="https://imgcache.gzonline.gov.cn/cos/iconspace_jz_5d9de18b.svg"></image>
            </view>
        </g-cell>
        <g-cell access="true" bind:tap="onTapBook" label="{{language.myBookings}}" wx:if="{{cityName!=='深圳市'&&!userInfo.isLeader&&language.isvisible}}">
            <view slot="prepend">
                <image class="mine__cell-image" src="https://imgcache.gzonline.gov.cn/cos/iconspace_verivied_159fd662.svg"></image>
            </view>
        </g-cell>
        <g-cell access="true" bind:tap="onTapEnterprise" label="{{language.correlationInformation}}" wx:if="{{language.isvisible}}">
            <view slot="prepend">
                <image class="mine__cell-image" src="https://imgcache.gzonline.gov.cn/cos/iconspace_verivied_159fd662.svg"></image>
            </view>
        </g-cell>
    </view>
    <view class="work" wx:if="{{role&&language.isvisible}}">
        <g-cell access="true" bind:tap="onJump" label="{{language.myWorkPlace}}">
            <view slot="prepend">
                <image class="mine__cell-image" src="https://imgcache.gzonline.gov.cn/cos/iconspace_work_122d6690.svg"></image>
            </view>
        </g-cell>
    </view>
    <view class="btn-row">
        <button bind:tap="onTapLogout" type="default">{{language.logout}}</button>
    </view>
    <view class="mine__list" wx:if="{{false}}">
        <g-cell access label="{{language.latestAnnouncement}}">
            <view slot="prepend">
                <image class="mine__cell-image" src="https://imgcache.gzonline.gov.cn/cos/iconspace_gg_ce5b4fc2.svg"></image>
            </view>
        </g-cell>
        <g-cell access label="{{language.volunteer}}">
            <view slot="prepend">
                <image class="mine__cell-image" src="https://imgcache.gzonline.gov.cn/cos/iconspace_zyz_20a923aa.svg"></image>
            </view>
        </g-cell>
        <g-cell access="true" catchtap="onTapFeedback" label="{{language.feedback}}">
            <view slot="prepend">
                <image class="mine__cell-image" src="https://imgcache.gzonline.gov.cn/cos/iconspace_ly_c50984d6.svg"></image>
            </view>
        </g-cell>
    </view>
    <g-footer fixed="{{!language.isvisible}}" sponsor="{{language.guangzhouDisease}}" supportor="{{language.supportor}}"></g-footer>
</block>
